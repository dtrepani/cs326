TDSmalltalkLeafNode {	#name : 'suite',	#contents : '| suite configs version classes testCases |suite := TestSuite named: \'Metacello Suite\'.configs := Array new.version := BaselineOfMetacello project version.classes := version currentlyLoadedClassesInVersion.testCases := classes select: [ :cls | cls inheritsFrom: TestCase ].testCases do: [ :testCase | suite addTests: testCase suite tests ].^ suite'}